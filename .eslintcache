react-movies-app/src/components/ui-components/movie/index.js",["131"],"import React, { useState } from \"react\"\nimport ImageApp from \"../image-app\";\nimport RankComponent from \"../rank/index\";\nimport RankWithState from \"../rank/rankWithState\";\nimport RankWithStateInput from \"../rank/rankWithStateInput\";\nimport { useHistory } from \"react-router-dom\"\n\nexport default function Movie(props) {\n    // this.state.isAdditionalInfoOpened  this.setState    // this.state = {isAdditionalInfoOpened:false}\n    const [isAdditionalInfoOpened, setAdditionalInfo] = useState(false)\n    const [cardColor, setCardColor] = useState(null)\n    const history = useHistory();\n\n    const showAdditionalInfo = () => {\n        return isAdditionalInfoOpened ? <div>\n            <div>\n                <div>Please Rank:</div>\n                <RankWithState />\n                <RankWithStateInput />\n            </div>\n        </div> : null\n    }\n\n    const additionalInfoAction = () => {\n        setAdditionalInfo(!isAdditionalInfoOpened)\n    }\n    const setCardColorAction = () => {\n        setCardColor(\"red\")\n    }\n\n    const { Title, Year, Poster, Type, imdbID, showImage, Rank } = props;\n    return (\n        <div style={{ background: cardColor }} className=\"card col-lg-3\">\n            {showImage && <ImageApp src={Poster} />}\n            <div className=\"card-body\">\n                <h5 className=\"card-title\">{Title}</h5>\n                <p className=\"card-text\">{Year}</p>\n\n                <p className=\"card-text\">{imdbID}</p>\n\n                <button\n                    onClick={() => {\n                        history.push(`/movie-page/${imdbID}`) // /movie-page/:movieTitle\n                    }}\n\n                    className=\"btn btn-primary\"\n                >\n                    Go to Movie\n                </button>\n                <div className=\"row mt-2\">\n                    <button className={\"btn btn-primary col-6\"} onClick={additionalInfoAction} > More Info </button>\n                    <button className={\"btn btn-primary col-6\"} onClick={setCardColorAction} > color </button>\n                </div>\n                <div className=\"row mt-2\">\n                    <RankComponent rank={Rank} />\n\n                </div>\n                <div className=\"row mt-2\">\n\n                </div>\n                {showAdditionalInfo()}\n            </div>\n        </div>\n    );\n}\n\n\n","/Users/galamouy/lectures/haifa20/react-movies-app/src/components/ui-components/rank/rankWithState.js",[],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/ui-components/rank/rankWithStateInput.js",["132"],"import React, { useState } from \"react\"\nimport { StarFill, Star } from 'react-bootstrap-icons';\n\n\n\n\nexport default function RankWithStateInput() {\n\n    const [numberOfStars, setNumberOfStars] = useState(-1)\n\n    const stars = _getStars()\n\n    return <div>\n        <input type={\"number\"} onChange={(event) => { setNumberOfStars(Number(event.target.value)) }} />\n        <span> {stars}</span>\n    </div>\n\n    function _getStars() {\n        const starsIcons = [];\n        for (let index = 0; index < numberOfStars; index++) {\n            starsIcons.push(<StarFill key={index} />)\n\n        }\n        return starsIcons;\n    }\n}\n\n\n\n","/Users/galamouy/lectures/haifa20/react-movies-app/src/components/ui-components/add-movie/index.js",[],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/ui-components/add-movie/index-class.js",["133"],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/pages/home/index.js",["134","135","136","137","138","139","140","141"],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/ui-components/movies-list/index.js",["142"],"import React from \"react\"\nimport Movie from \"../movie\";\nimport MovieCardClass from \"../movie/index-class\";\n\n\n\nexport default function MoviesList(props) {\n    const { movies = [], showImage } = props;\n    if (!Array.isArray(movies)) return <div> Movies List is not Availble </div>;\n    return movies.map((movie) => {\n        return <Movie key={movie.imdbID} showImage={showImage} {...movie} />;\n    });\n}","/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/hoc/isLoading/index.js",[],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/ui-components/nav-bar/index.js",["143","144","145"],"import React from \"react\"\nimport { Nav } from \"react-bootstrap\"\n\nimport { Link } from \"react-router-dom\"\nimport AboutPage from \"../../containers/pages/about\"\nimport HomePage from \"../../containers/pages/home\"\nimport AddMovie from \"../add-movie\"\nimport css from \"./style.module.css\"\n\nexport const routes = [\n    { to: \"/\", title: \"Home\" },\n    { to: \"/add-movie\", title: \"Add Movie\" },\n    { to: \"/about-page\", title: \"About\" },\n    { to: \"/favorites-page\", title: \"Favorites\" },\n    { to: \"/home2\", title: \"home2\" }]\n\nexport default function NavBarApp() {\n    return <Nav\n        variant=\"pills\" className={css.mainNavBar}>\n        <NavItmesList links={routes} />\n    </Nav>\n}\n\nfunction NavItmesList(props) {\n\n    const { links } = props\n    if (!Array.isArray(links)) return null\n    return links.map(link => {\n        return <Nav.Item>  <Link className={css.customLink} to={link.to} >{link.title}</Link> </Nav.Item>\n    })\n}","/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/pages/about/index.js",[],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/pages/movie-page/index.js",["146","147","148"],"import React, { useEffect, useState } from \"react\"\nimport { Link, useParams, useHistory } from \"react-router-dom\"\nimport axios from \"axios\"\nimport { API_URL } from \"../home/index\"\nimport WithLoading from \"../../hoc/isLoading\"\nimport { useDispatch } from \"react-redux\"\nimport { ACTIONS } from \"../../../../redux-store/actions\"\n\n\nexport default function MoviePage() {\n    const params = useParams()\n    const [movie, setMovie] = useState(null)\n    const [isLoading, setIsLoading] = useState(true)\n\n    useEffect(() => {\n        getMovieDetails(params.id)\n    }, [])\n\n    async function getMovieDetails(id) {\n\n        try {\n            const { data } = await axios.get(`${API_URL}&i=${id}`)\n            setMovie(data)\n        } catch (ex) {\n            // print some error to the UI\n            console.log(\"we had an error\", ex)\n        } finally {\n            setIsLoading(false)\n        }\n    }\n\n    const MovieWithLoading = WithLoading(MovieComponent)\n    return <div> Movie Page: {params.id}\n\n        <MovieWithLoading isLoading={isLoading} movie={movie} />\n    </div>\n}\n\nfunction MovieComponent(props) {\n    const history = useHistory()\n    const dispatch = useDispatch()\n    if (!props.movie) return null;\n    const { Poster, Country } = props.movie\n    return <div>\n        <h2><Link to={`/country-page/${Country}`} > {Country}</Link>  </h2>\n        {/* <h2 onClick={() => { history.push(`/country-page/${Country}`) }}> {Country}  </h2> */}\n\n        <img src={Poster} width={400} height={400} />\n        <button onClick={() => { dispatch({ type: ACTIONS.MOVIES.ADD_TO_FAVORITES, payload: props }) }}\n            className=\"btn btn-primary\"> Add To Favorites  </button>\n    </div>\n}","/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/pages/country-page/index.js",["149","150"],"import React, { useEffect, useState } from \"react\"\nimport { useParams } from \"react-router-dom\"\nimport axios from \"axios\"\nimport WithLoading from \"../../hoc/isLoading\"\n\nconst API_COUNTRIES_URL = \"https://restcountries.eu/rest/v2/name\"\n\nexport default function CountryPage() {\n    const params = useParams()\n    const { country: countryParam } = params;\n    const firstCountry = getFirstCountry(countryParam)\n    function getFirstCountry(country) {\n        const [firstCountry] = country.split(\",\")\n        return firstCountry\n    }\n    const [country, setCountry] = useState(null)\n    const [isLoading, setIsLoading] = useState(true)\n\n    useEffect(() => {\n        getCountryDetails(firstCountry)\n    }, [])\n\n    async function getCountryDetails(country) {\n        try {\n            const { data } = await axios.get(`${API_COUNTRIES_URL}/${country}`)\n            setCountry(data[0] || data)\n        } catch (ex) {\n            console.log(ex)\n        } finally {\n            setIsLoading(false)\n        }\n    }\n\n    const CountryWithLoading = WithLoading(CountryComponent)\n    return <div>\n        <CountryWithLoading isLoading={isLoading} country={country} />\n    </div>\n}\n\nfunction CountryComponent(props) {\n    if (!props.country) return null\n    return <div>\n        <img src={props.country.flag} style={{ border: \"1px solid black\" }} />\n    </div>\n}","/Users/galamouy/lectures/haifa20/react-movies-app/src/redux-store/index.js",[],"/Users/galamouy/lectures/haifa20/react-movies-app/src/redux-store/actions.js",[],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/pages/favorites-page/index.js",["151","152","153","154","155","156","157","158","159","160"],"import React, { useEffect, useState } from \"react\"\nimport { Link, useParams, useHistory } from \"react-router-dom\"\nimport axios from \"axios\"\nimport { API_URL } from \"../home/index\"\nimport WithLoading from \"../../hoc/isLoading\"\nimport { useDispatch, useSelector } from \"react-redux\"\nimport { ACTIONS } from \"../../../../redux-store/actions\"\nimport MoviesList from \"../../../ui-components/movies-list\"\n\n\nexport default function FavoritesPage() {\n    const favorites = useSelector(state => state.favorites)\n    return <div className=\"container\">\n        <h1> Favorites </h1>\n        <div className=\"row\">\n            <MoviesList movies={favorit