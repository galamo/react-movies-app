te([])\n    const [inputValue, setInputValue] = useState(null)\n    const [searchBy, setSearchBy] = useState(defaultItem.value)\n    const [errorMessage, setErrorMessage] = useState(null)\n    const [isLoading, setIsLoading] = useState(false)\n    // useEffect\n    // param 1 - function to execute\n    // param 2 - dependencies array\n    useEffect(() => {\n        callApi()\n    }, [inputValue, searchBy])\n\n    async function callApi() {\n        if (!inputValue) return;\n        setIsLoading(true)\n        const { data } = await axios.get(`${API_URL}&${searchBy}=${inputValue}`)\n        const { Response, Error: errorMessage } = data;\n        if (Response === \"False\") {\n            setErrorMessage(errorMessage)\n        } else {\n            setErrorMessage(null)\n            setMovies(data.Search || [data])\n        }\n        setIsLoading(false)\n    }\n\n    const MoviesListWithLoading = WithLoading(MoviesList)\n    return <div className=\"container\">\n\n        {_getAlert(errorMessage)}\n        <div className=\"row mt-3 mb-3\">\n            <div className=\"col-lg-9 p-0\">\n                <InputGroup >\n                    <FormControl onChange={(e) => { setInputValue(e.target.value) }}\n                        placeholder=\"Search Movie\"\n                    />\n                </InputGroup>\n            </div>\n            <div className=\"col-lg-3 p-0\">\n                <Form.Control as=\"select\" onChange={(e) => { setSearchBy(e.target.value) }} custom>\n                    <GetOptions options={params} defaultItem={defaultItem} />\n                </Form.Control>\n            </div>\n        </div>\n        <div className=\"row\">\n            <MoviesListWithLoading isLoading={isLoading} movies={movies} showImage={true} />\n        </div>\n    </div>\n\n    function _getAlert(message) {\n        return (<div className=\"row\">\n            <Alert show={message} style={{ textAlign: \"center\", position: \"absolute\", left: \"0px\", top: \"0px\", width: \"100%\" }} key={123456789} variant={\"danger\"}>\n                {message}\n            </Alert>\n        </div>)\n    }\n}\n\nfunction GetOptions(props) {\n    const { options, defaultItem } = props;\n    if (!Array.isArray(options)) return []\n    return params.map((optionItem) => {\n        const isSelected = optionItem.value === defaultItem.value ? \"selected\" : null\n        return <option selected={isSelected} key={optionItem.key} value={optionItem.value}>{optionItem.innerText} </option>\n    })\n}","/Users/galamouy/lectures/haifa20/react-movies-app/src/components/ui-components/movies-list/index.js",["104"],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/hoc/isLoading/index.js",[],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/ui-components/nav-bar/index.js",["105","106","107"],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/pages/about/index.js",[],"/Users/galamouy/lectures/haifa20/react-movies-app/src/components/containers/pages/movie-page/index.js",[],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":3,"column":35,"nodeType":"114","messageId":"115","endLine":3,"endColumn":39},{"ruleId":"112","severity":1,"message":"116","line":4,"column":10,"nodeType":"114","messageId":"115","endLine":4,"endColumn":16},{"ruleId":"112","severity":1,"message":"117","line":5,"column":8,"nodeType":"114","messageId":"115","endLine":5,"endColumn":22},{"ruleId":"112","severity":1,"message":"118","line":11,"column":10,"nodeType":"114","messageId":"115","endLine":11,"endColumn":16},{"ruleId":"112","severity":1,"message":"119","line":49,"column":5,"nodeType":"114","messageId":"115","endLine":49,"endColumn":20},{"ruleId":"108","replacedBy":"120"},{"ruleId":"110","replacedBy":"121"},{"ruleId":"122","severity":1,"message":"123","line":10,"column":9,"nodeType":"124","endLine":10,"endColumn":81},{"ruleId":"112","severity":1,"message